{
  "name": "loandispatcher",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "env-cmd .env npm run codegen:env",
    "codegen:env": "graph codegen $THEGRAPH_MANIFEST",
    "build": "env-cmd .env npm run build:env",
    "build:env": "graph build $THEGRAPH_MANIFEST",
    "authorize": "graph auth https://api.thegraph.com/deploy/ ",
    "deploy": " env-cmd .env npm run deploy:env",
    "deploy:env": "graph deploy --debug --access-token $THEGRAPH_KEY --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ $THEGRAPH_NAME $THEGRAPH_MANIFEST",
    "create-local": "env-cmd .env graph create --node http://localhost:8020/ $THEGRAPH_NAME",
    "remove-local": "env-cmd .env graph remove --node http://localhost:8020/ $THEGRAPH_NAME",
    "deploy-local": "env-cmd .env graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 $THEGRAPH_NAME"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.13.3",
    "@graphprotocol/graph-ts": "0.13.0",
    "env-cmd": "8.0.2"
  }
}
